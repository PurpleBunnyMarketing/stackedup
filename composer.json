{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "abraham/twitteroauth": "^4.0",
        "barryvdh/laravel-debugbar": "^3.3",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "facebook/graph-sdk": "^5.7",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.12.1",
        "google/apiclient-services": "^0.301.0",
        "googleads/google-ads-php": "^19.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "hesto/multi-auth": "^2.0",
        "intervention/image": "^2.7",
        "laracasts/flash": "^3.1",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.2",
        "laravel/passport": "^10.4",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "pawlox/video-thumbnail": "^1.0",
        "pbmedia/laravel-ffmpeg": "^7.8",
        "php-ffmpeg/php-ffmpeg": "^0.19.0",
        "predis/predis": "^1.1",
        "socialiteproviders/instagram": "^4.1",
        "socialiteproviders/instagram-basic": "^4.1",
        "stripe/stripe-php": "^9.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.3",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "jn-jairo/laravel-ngrok": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeds/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
